                "data.overfit=1",
                "model.PREDICT_TYPE=sample",
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
                "data.val_batch_size=1",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "test-overfitting",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "args": [
                "experiment_name=overfitting_test",
                "model.ref_part=True",
                "data.overfit=1",
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
                "data.val_batch_size=1",
                "inference_dir=rot_only",
                "ckpt_path=output/overfitting_test/training/last-v2.ckpt"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "args": [
                "experiment_name=train_all_bs24_4gpu_5000iters_fixed_encoder_add_ref_part",
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
                "inference_dir=given_gt_rots",
                "ckpt_path=output/train_all_bs24_4gpu_5000iters_fixed_encoder_add_ref_part/training/last.ckpt",
                "model.num_inference_steps=100",
                "data.val_batch_size=6",
                "model.ref_part=True",
                "model.gt_rots=True"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },

        {
            "name": "visualize forward process",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/vis_forward_process.py",
            "args": [
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
                // "model.BETA_END=0.001286",
                // "data.overfit=1",
                "+save_dir=forward_translation/custom_0.1_fixed",
