    "version": "0.2.0",
    "configurations": [
        {
            "name": "overfitting",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "args": [
                "experiment_name=overfitting_new_architecture",
                "model.ref_part=True",
                "data.overfit=1",
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },

        {
            "name": "test-ref-overfitting",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "args": [
                "experiment_name=overfitting_ref_part",
                "model.ref_part=True",
                "data.overfit=1",
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
                "data.val_batch_size=1",
                "inference_dir=overfiting_test",
                "ckpt_path=output/overfitting_ref_part/training/last.ckpt"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test.py",
            "args": [
                "experiment_name=train_all_rot_only_rot_every_batch",
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
                "inference_dir=test_results",
                "ckpt_path=output/train_all_rot_only_rot_every_batch/training/epoch1899.ckpt",
                "model.num_inference_steps=100",
                "data.rot_in_getitem=True",
                "data.val_batch_size=50",
                "model.ref_part=True",
                "data.data_dir=../vq_jigsaw/latent_data/bottle_volume_constrained_no_rot/train/",
                "data.data_val_dir=../vq_jigsaw/latent_data/bottle_volume_constrained_no_rot/val/",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },

        {
            "name": "visualize forward process",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/utils/vis_forward_process.py",
            "args": [
                "model.DDPM_BETA_SCHEDULE=squaredcos_cap_v2",
                // "model.BETA_END=0.001286",
                // "data.overfit=1",
                "+save_dir=forward_translation/custom_0.1_fixed",
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },

        {
            "name": "render results",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vis_results.py",
            "args": [
                "experiment_name=train_all_bs24_4gpu_5000iters_fixed_encoder_add_ref_part",
                "inference_dir=test_results",
                // "+trainer.overfit_batches=1"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
}